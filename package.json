{
  "name": "gulp-jsonmerge",
  "version": "1.0.0",
  "description": "A plugin for Gulp to merge several JSON files",
  "main": "src/index.js",
  "scripts": {
    "test": "istanbul test _mocha --report html -- test/*.js --reporter spec",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eyolas/gulp-jsonmerge.git"
  },
  "keywords": [
    "gulp",
    "json"
  ],
  "author": "David Touzet <dtouzet@gmail.com>",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/eyolas/gulp-jsonmerge/issues"
  },
  "homepage": "https://github.com/eyolas/gulp-jsonmerge#readme",
  "dependencies": {
    "gulp-util": "^3.0.6",
    "lodash": "^3.10.1",
    "through2": "^2.0.0"
  },
  "devDependencies": {
    "mocha": "*",
    "coveralls": "*",
    "mocha-lcov-reporter": "*",
    "istanbul": "*",
    "event-stream": "*",
    "should": "~2.1.0"
  }
}
